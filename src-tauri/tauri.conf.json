{
    "$schema": "https://schema.tauri.app/config/2",
    "productName": "yys1",
    "version": "0.1.0",
    "identifier": "com.yys1.app",
    "build": {
        "beforeDevCommand": "bun run dev",
        "devUrl": "http://localhost:1420",
        "beforeBuildCommand": "bun run build",
        "frontendDist": "../dist"
    },
    "app": {
        "windows": [
            {
                "title": "yys1",
                "width": 800,
                "height": 600
            }
        ],
        "security": {
            "csp": null
        }
    },
    "bundle": {
        "active": true,
        "targets": "all",
        "icon": [
            "icons/icon.ico",
            "icons/icon.png",
            "icons/32x32.png",
            "icons/128x128.png",
            "icons/128x128@2x.png",
            "icons/icon.icns"
        ],
        "windows": {
            "webviewInstallMode": {
                "type": "embedBootstrapper"
            }
        }
    }
}